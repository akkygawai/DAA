def subset_sum(arr, target, i=0, curr=[]):
    # If current subset sums to target, print solution
    if sum(curr) == target:
        print("Solution:", curr)
        return

    # Stop if we've considered all elements or exceeded target
    if i == len(arr) or sum(curr) > target:
        return

    # Include arr[i] in the subset
    subset_sum(arr, target, i + 1, curr + [arr[i]])
    # Exclude arr[i] from the subset
    subset_sum(arr, target, i + 1, curr)


# Input
n = int(input("Enter number of elements: "))
arr = [int(input()) for _ in range(n)]
target = int(input("Enter target sum: "))

# Run subset sum
subset_sum(arr, target)
