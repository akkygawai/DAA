def obst(prob, n):
    # dp[i][j] will store the minimum cost of OBST for keys i..j
    dp = [[0] * n for _ in range(n)]
    # Base case: single key
    for i in range(n):
        dp[i][i] = prob[i]
    # Consider chains of length 2 to n
    for length in range(2, n + 1):
        for i in range(n - length + 1):
            j = i + length - 1
            dp[i][j] = float('inf')
            # Try making each key in [i..j] the root
            for r in range(i, j + 1):
                left = dp[i][r - 1] if r > i else 0
                right = dp[r + 1][j] if r < j else 0
                cost = left + right + sum(prob[i:j + 1])
                dp[i][j] = min(dp[i][j], cost)
    return dp[0][n - 1]
n = int(input("Enter number of keys: "))
prob = [float(input()) for _ in range(n)]

print("Minimum cost:", obst(prob, n))
