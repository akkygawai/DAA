n = int(input("Enter number of elements: "))
arr = []

for i in range(n):
    arr.append(int(input()))

mid = n // 2

# Initialize min1 and max1 with the first element
min1 = max1 = arr[0]

# Find min and max in the first half
for i in range(0, mid):
    if arr[i] < min1:
        min1 = arr[i]
    if arr[i] > max1:
        max1 = arr[i]

# Initialize min2 and max2 with the first element of the second half
min2 = max2 = arr[mid]

# Find min and max in the second half
for i in range(mid, n):
    if arr[i] < min2:
        min2 = arr[i]
    if arr[i] > max2:
        max2 = arr[i]

# Compare results from both halves
minimum = min1 if min1 < min2 else min2
maximum = max1 if max1 > max2 else max2

print("Minimum:", minimum)
print("Maximum:", maximum)
